<?xml version="1.0"?>

<project name="ISBApps" default="compile" basedir=".">

  <target name="compile">
    <ant dir="${basedir}/Dizzy" target="compile" />
  </target>

  <target name="clean">
    <ant dir="${basedir}/Dizzy" target="clean" />
  </target>

  <target name="distclean" depends="clean">
  </target>

  <target name="buildWeb">
    <ant dir="${basedir}/Dizzy" target="buildWeb" />
  </target>

  <target name="build">
    <ant dir="${basedir}/Dizzy" target="build" />
  </target>

</project>
